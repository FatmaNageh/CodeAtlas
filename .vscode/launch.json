{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run CodeAtlas Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/apps/extension"],
      "outFiles": ["${workspaceFolder}/apps/extension/dist/*.cjs"],
      "preLaunchTask": "build-extension"
    }
  ]
}
